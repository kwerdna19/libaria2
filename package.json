{
  "name": "libaria2",
  "version": "1.0.91",
  "description": "A full-typing wrapper around aria2`, `The next generation download utility`.",
  "keywords": [
    "aria2",
    "download",
    "http",
    "websocket",
    "metalink",
    "magnet",
    "ftp",
    "bittorrent",
    "typescript"
  ],
  "homepage": "https://hydrati.github.io/libaria2-ts/",
  "repository": "https://github.com/yjl9903/libaria2",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "build:docs": "typedoc --tsconfig ./tsconfig.json ./src/lib.ts",
    "build:dts": "api-extractor run",
    "lint": "eslint -c ./.eslintrc",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "ts-node ./tests/index.ts"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.18.4",
    "@types/events": "^3.0.0",
    "@types/node": "^16.4.13",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "events": "^3.3.0",
    "isomorphic-ws": "^4.0.1",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "packageManager": "pnpm@8.4.0"
}